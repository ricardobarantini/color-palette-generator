<html>
  <head>
    <title>Color Palette Generator</title>
    <style>
    .grid-container {
        display: grid;
        grid-template-columns: repeat(5, 250px);
        grid-gap: 10px;
      }
    </style>
  </head>

  <body>
    <h1>Color Palette Generator</h1>

    <div class="grid-container">
      {{ colors }}
    </div>
  </body>
</html>

